doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title Users
        link(rel="stylesheet", href="/css/styles.css")
    body 
        include ./header
        h1 aqui va lista que vera el administrador con los usuarios registrados
            each user in result
                p #{user.user_name}
                p #{user.user_email}
                p #{user.user_password}
                p #{user.user_age}
                p #{user.user_dni}
                hr
        script(src="/js/script.js")